{
	"name": "Admin-Color-Schemes",
	"slug": "admin-color-schemes",
	"description": "WordPress dashboard color schemes",
	"version": "0.1.0",
	"devDependencies": {
		"gulp": "4.0.2",
		"gulp-concat": "2.6.1",
		"gulp-csso": "4.0.1",
		"gulp-dart-sass": "1.0.2",
		"gulp-rename": "2.0.0",
		"gulp-rtlcss": "1.4.1",
		"stylelint": "13.8.0",
		"stylelint-config-wordpress": "17.0.0"
	},
	"stylelint": {
		"extends": [
			"stylelint-config-wordpress/scss"
		],
		"rules": {
			"no-descending-specificity": null
		},
		"ignoreFiles": [
			"_*.scss"
		]
	},
	"scripts": {
		"start": "gulp",
		"build": "gulp build",
		"lint": "stylelint */*.scss"
	}
}
